{
  "hostName": "home-cloud",
  "domain": "local",
  "networkmanager": {
    "enable": true
  },
  "wireless": {
    "enable": false
  },
  "firewall": {
    "enable": false
  },
  "nat": {
    "enable": true,
    "externalInterface": "eth0",
    "internalInterfaces": [
      "wg0"
    ]
  },
  "wireguard": {
    "interfaces": {
      "wg0": {
        "ips": [
          "10.100.0.1/24"
        ],
        "listenPort": 51820,
        "privateKeyFile": "/etc/home-cloud/wireguard-keys/private",
        "peers": []
      }
    }
  }
}
